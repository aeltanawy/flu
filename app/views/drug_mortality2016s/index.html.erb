<p id="notice"><%= notice %></p>

<h1>Drug Mortality 2016</h1>

<table class="table table-striped table-hover table-responsive">
  <thead>
    <tr>
      <th>State</th>
      <th>Deaths</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @drug_mortality2016s.each do |drug_mortality2016| %>
      <tr>
        <td><%= drug_mortality2016.state.name %></td>
        <td><%= drug_mortality2016.deaths %></td>
        <td><%= link_to 'Show', drug_mortality2016 %></td>
        <td><%= link_to 'Edit', edit_drug_mortality2016_path(drug_mortality2016) %></td>
        <td><%= link_to 'Destroy', drug_mortality2016, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Drug Mortality 2016', new_drug_mortality2016_path %>
